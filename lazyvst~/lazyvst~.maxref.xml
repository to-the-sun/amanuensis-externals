	<!--ARGUMENTS-->
	<!--MESSAGES-->
	<methodlist>
		<method name="plug">
			<digest>Load a plugin path</digest>
			<description>Accepts a symbol representing a path to a plugin. Currently only logs the path if logging is enabled.</description>
		</method>
		<method name="dsp64">
